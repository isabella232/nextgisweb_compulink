<map>
  <layer>
    <type>Line</type>

    <class>
      <expression>( '[laying_method]'='ground' )</expression>
      <style>
        <linecap>square</linecap>
        <linejoin>bevel</linejoin>
        <color red="156" green="121" blue="0" />
        <width>3.0</width>
      </style>
    </class>

    <class>
      <expression>( '[laying_method]'='transmission_towers' )</expression>
      <style>
        <linecap>square</linecap>
        <linejoin>bevel</linejoin>
        <color red="0" green="0" blue="0" />
        <width>3.0</width>
      </style>
    </class>

    <class>
      <expression>( '[laying_method]'='canalization' )</expression>
      <style>
        <linecap>square</linecap>
        <linejoin>bevel</linejoin>
        <color red="255" green="139" blue="0" />
        <width>3.0</width>
      </style>
    </class>

    <class>
      <expression>( '[laying_method]'='building' )</expression>
      <style>
        <linecap>square</linecap>
        <linejoin>bevel</linejoin>
        <color red="29" green="66" blue="201" />
        <width>3.0</width>
      </style>
    </class>

    <class>
      <expression>( '[laying_method]'='other' )</expression>
      <style>
        <linecap>square</linecap>
        <linejoin>bevel</linejoin>
