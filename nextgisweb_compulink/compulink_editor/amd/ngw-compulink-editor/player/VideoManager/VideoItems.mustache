<ul class="video-items">
    {{#videoInfoItems}}
        <li>
            <span class="caption {{#status_error}}red{{/status_error}}">{{name}} {{#size}}[{{size}}]{{/size}}</span>
            {{#status_creating}}
                <a class="icon creating" href="javascript:void(0)" title="Создается"></a>
            {{/status_creating}}
            {{#status_ready}}
                <a class="icon remove-video-item icon-cancel-circled2" data-id="{{id}}" data-name="{{name}}" href="javascript:void(0)" title="Удалить"></a>
                <a class="icon icon-download-alt" {{#url}}href="{{url}}"{{/url}} {{^url}}href="javascript:void(0)"{{/url}} title="Скачать"></a>
            {{/status_ready}}
            {{#status_error}}
                <a class="icon remove-video-item icon-cancel-circled2 red" data-id="{{id}}" data-name="{{name}}" href="javascript:void(0)" title="Удалить"></a>
                <a class="icon icon-attention red" href="javascript:void(0)" title="Ошибка при создании"></a>
            {{/status_error}}
        </li>
    {{/videoInfoItems}}
</ul>